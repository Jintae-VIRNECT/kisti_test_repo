  "description": "Virnect Platofrm Login FrontEnd",
  "author": "SungHyun Kang <k23sh138@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env VIRNECT_ENV=local NODE_ENV=local webpack-dev-server --inline --hot --config build/webpack.dev.conf.js",
    "dev:onpremise": "cross-env VIRNECT_ENV=onpremise NODE_ENV=onpremise webpack-dev-server --inline --hot --config build/webpack.dev.conf.js",
    "build": "webpack --progress --config build/webpack.prod.conf.js",
    "start:local": "cross-env VIRNECT_ENV=local NODE_ENV=local node server.js",
    "start": "node server.js",
    "lint": "eslint --fix src/**",
    "i18n": "i18n --sheetId PF-Login",
    "test": "jest",
    "cy:open-dev": "cypress open --env configFile=develop",
    "cy:run-dev": "cypress run --env configFile=develop",
    "cy:report-dev": "yarn cy:clean; cypress run --env configFile=develop",
    "prepare": "husky install"
  },
