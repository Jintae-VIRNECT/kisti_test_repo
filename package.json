{
  "name": "workstation",
  "version": "1.3.8.3",
  "description": "Virnect Workstation",
  "author": "SangBaek-VIRNECT <lsb@virnect.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env VIRNECT_ENV=local nuxt",
    "dev:onpremise": "cross-env VIRNECT_ENV=onpremise nuxt",
    "build:develop": "cross-env NODE_ENV=develop VIRNECT_ENV=local nuxt build --modern=server",
    "build:production": "cross-env NODE_ENV=production VIRNECT_ENV=local nuxt build --modern=server",
    "deploy": "cross-env NODE_ENV=$VIRNECT_ENV VIRNECT_ENV=$VIRNECT_ENV CONFIG_SERVER=$CONFIG_SERVER nuxt start --modern=server",
    "e2e": "cypress",
    "i18n": "i18n --sheetId PF-WebWorkstation",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.10.3",
    "@nuxtjs/style-resources": "^1.0.0",
    "@virnect/components": "^1.8.0",
    "@virnect/logger": "^1.0.2",
    "@virnect/platform-auth": "^1.0.2",
    "@virnect/ui-assets": "^1.6.0",
    "axios": "^0.19.2",
    "billboard.js": "^1.12.10",
    "cross-env": "^7.0.2",
    "dayjs": "^1.8.23",
    "dotenv": "^8.2.0",
    "element-ui": "^2.13.0",
    "js-cookie": "^2.2.1",
    "nuxt": "^2.0.0",
    "nuxt-helmet": "^2.0.0",
    "nuxt-i18n": "6.21.1"
  },
  "devDependencies": {
    "@virnect/eslint-config": "^1.0.0",
    "@virnect/i18n": "^1.1.1",
    "@virnect/stylelint-config": "^1.0.1",
    "@vue/component-compiler-utils": "^3.1.0",
    "cypress": "^7.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "husky": "^6.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.0.2",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2"
  }
}
