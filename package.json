{
  "name": "rm-dashboard",
  "version": "2.3.1-0",
  "private": true,
  "description": "rm-dashboard",
  "author": "ykmo@virnect.com",
  "scripts": {
    "serve": "npm run lint && vue-cli-service serve",
    "build": "vue-cli-service lint && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:develop": "vue-cli-service build --mode development",
    "report": "vue-cli-service build --report",
    "start": "node server.js",
    "start:develop": "set CONFIG_SERVER=http://192.168.6.3:6383 && set VIRNECT_ENV=develop && set NODE_ENV=develop && node server.js",
    "start:op": "set CONFIG_SERVER=http://192.168.6.3:6383 && set VIRNECT_ENV=onpremise && set NODE_ENV=production && node server.js",
    "start:production": "set CONFIG_SERVER=https://config.virnect.com && set VIRNECT_ENV=production && set NODE_ENV=production && node server.js",
    "start:staging": "set CONFIG_SERVER=https://stgconfig.virnect.com && set VIRNECT_ENV=staging && set NODE_ENV=production && node server.js",
    "test:cover": "vue-cli-service test:unit --coverage",
    "i18n:diff": "i18n diff --sheetId RM-DashBoard --src src/languages/remote",
    "i18n:upload": "i18n diff --sheetId RM-DashBoard --src src/languages/remote --force",
    "i18n:download": "i18n download --sheetId RM-DashBoard --src src/languages/remote",
    "i18n": "i18n --sheetId RM-DashBoard --src src/languages/remote",
    "prepare": "husky install"
  },
  "dependencies": {
    "@virnect/logger": "^1.0.2",
