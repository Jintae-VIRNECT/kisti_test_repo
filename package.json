{
  "name": "remote-webclient",
  "version": "2.0.0",
  "description": "remote-webclient",
  "author": "delbert@virnect.com",
  "private": true,
  "bin": {
    "app": "./server.js"
  },
  "pkg": {
    "assets": [
      "dist/**/*"
    ]
  },
  "scripts": {
    "build:develop": "export NODE_ENV=develop || set NODE_ENV=develop && webpack --progress --config build/webpack.develop.conf.js",
    "build:production": "export NODE_ENV=production || set NODE_ENV=production && webpack --progress --config build/webpack.production.conf.js",
    "start:local": "export NODE_ENV=local || set NODE_ENV=local&& export SSL_ENV=private || set SSL_ENV=private&& webpack-dev-server --progress --inline --hot --config build/webpack.local.conf.js",
    "start:develop": "export NODE_ENV=develop || set NODE_ENV=develop && export SSL_ENV=private || set SSL_ENV=private&&node server.js",
    "start:production": "export NODE_ENV=production || set NODE_ENV=production && export SSL_ENV=public || set SSL_ENV=public&&node server.js",
    "start:pkg": "pkg ."
  },
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "dotenv": "^8.2.0",
    "element-ui": "^2.13.0",
    "eslint-plugin-vue": "^5.2.3",
    "express": "^4.17.1",
    "vue": "^2.6.11",
    "vue-moment": "^4.1.0",
    "vue-raven": "^1.0.3",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^1.0.1",
    "es6-promise": "^4.2.8",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^3.2.0",
    "log4js": "^6.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pkg": "^4.4.2",
    "sass-loader": "^7.3.1",
    "secure-web-storage": "^1.0.2",
    "style-loader": "^0.18.2",
    "url-loader": "^1.1.2",
    "vue-axios": "^2.1.5",
    "vue-i18n": "^8.15.3",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "openvidu-browser": "^2.11.0",
    "vue2-scrollbar": "^0.0.3"
  },
  "lint": "eslint --ext .js,.vue src"
}
